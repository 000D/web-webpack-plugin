{
  "name": "web-webpack-plugin",
  "version": "1.0.7",
  "description": "web plugin for webpack",
  "repository": {
    "type": "git",
    "url": "git@github.com:gwuhaolin/web-webpack-plugin.git"
  },
  "main": "index.js",
  "scripts": {
    "demo:auto-plugin": "cd ./demo/auto-plugin/ && webpack && cd ../../",
    "demo:config-resource": "cd ./demo/config-resource/ && webpack && webpack --config webpack.config-template.js && cd ../../",
    "demo:out-html": "cd ./demo/out-html/ && webpack && cd ../../",
    "demo:use-template": "cd ./demo/use-template/ && webpack && cd ../../",
    "demo": "npm run demo:auto-plugin && npm run demo:config-resource && npm run demo:out-html && npm run demo:use-template && cd ../../"
  },
  "author": "halwu",
  "license": "ISC",
  "dependencies": {
    "clone": "^2.1.0",
    "parse5": "^3.0.1"
  },
  "peerDependencies": {
    "webpack": "^2.2.0-rc.2",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}
